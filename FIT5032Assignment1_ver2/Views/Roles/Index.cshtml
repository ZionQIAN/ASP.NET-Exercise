@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>All Roles</h2>

<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { roleName = item.Name }) |
                @Html.ActionLink("Delete", "Delete", new { roleName = item.Name })
            </td>
        </tr>
    }

</table>