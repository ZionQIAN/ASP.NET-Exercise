@model IEnumerable<FIT5032Assignment1_ver2.Models.LoginedCourse>
@{
    ViewBag.Title = "UserLoginCourse";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UserLoginCourse</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationUser.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Course.CourseName)
            </th>
       

            <th></th>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.ApplicationUser.UserName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Course.CourseName)
    </td>


    </tr>}
    </tbody>
</table>

@section Scripts {

    @Scripts.Render("~/Scripts/DataTables/jquery.dataTables.js")
    @Scripts.Render("~/Scripts/DataTables/dataTables.bootstrap.js")
    <script>$(document).ready(function () {
            $('.table').DataTable();
        });</script>
}
<link href="~/Content/DataTables/css/dataTables.bootstrap.min.css"
      rel="stylesheet" />
